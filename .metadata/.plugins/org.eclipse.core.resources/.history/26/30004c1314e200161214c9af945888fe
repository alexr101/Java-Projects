package MergeSort;

public class MergeSort {
	public void merge(int[] arr, int start, int end){
		if(end < start){
			return;
		}
		
		int mid = (start + end)/2;
		
		merge(arr, start, mid-1);
		merge(arr, mid, end);
		sort(arr, start, mid, end);
	}
	
	public void sort(int[] arr, int start, int mid, int end){
		
	}
}
